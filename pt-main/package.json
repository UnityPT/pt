{
  "name": "pt",
  "version": "1.0.7",
  "description": "",
  "type": "module",
  "main": "dist/main/bundle.cjs",
  "scripts": {
    "start": "webpack --mode development && electron .",
    "build": "webpack --mode production",
    "auto": "node --loader ts-node/esm pt/src/index.ts",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "upload": "npm run build && electron-builder --publish always"
  },
  "build": {
    "productName": "UnityPT",
    "appId": "com.upt.upt",
    "files": [
      "dist/main",
      "dist/pt-web",
      "package.json"
    ],
    "win": {
      "verifyUpdateCodeSignature": false,
      "icon": "logo.ico"
    },
    "linux": {
      "target": "AppImage"
    },
    "mac": {
      "icon": "icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "hardenedRuntime": true,
      "entitlements": "./node_modules/electron-builder-notarize/entitlements.mac.inherit.plist"
    },
    "afterSign": "electron-builder-notarize",
    "publish": {
      "provider": "s3",
      "endpoint": "https://ptdl.lolo.moe",
      "bucket": "unity-pt",
      "path": "downloads"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ctrl/qbittorrent": "^5.1.0",
    "@types/create-torrent": "^5.0.0",
    "@types/glob": "^8.0.0",
    "@types/klaw": "^3.0.3",
    "@types/parse-torrent": "^5.8.4",
    "@types/ssh2": "^1.11.6",
    "@types/tough-cookie": "^4.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "create-torrent": "^6.0.2",
    "electron": "^23.1.0",
    "electron-builder": "^24.0.0-alpha.12",
    "electron-builder-notarize": "^1.5.1",
    "electron-fetch": "^1.9.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "form-data": "^4.0.0",
    "glob": "^8.0.3",
    "klaw": "^4.0.1",
    "node-loader": "^2.0.0",
    "node-ssh": "^13.0.0",
    "p-retry": "^5.1.2",
    "parse-torrent": "^11.0.7",
    "rxjs": "^7.6.0",
    "tough-cookie": "^4.1.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typed-struct": "^2.3.0",
    "typescript": "^4.9.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "ssh2": "^1.11.0"
  }
}
